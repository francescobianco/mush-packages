#!/usr/bin/env bash
## BP010: Release metadata
## @build_date: 2023-10-13T13:46:29Z
set -e
extern() {
  extern=$1
}
legacy() {
  legacy=$1
}
module() {
  module=$1
}
public() {
  public=$1
}
use() {
  use=$1
}
embed() {
  embed=$1
}
## BP004: Compile the entrypoint

main() {
  if [ "$#" -eq 0 ]; then
    echo "Usage: echofake <command>"
    return 1
  fi

  script -q /dev/null -c "$*" 2>&1 | while read -r line; do
    line=$(echo -n "$line" | tr -d '\r' | cat -A - |
      sed 's/`/\\`/g' | sed 's/"/\\"/g' | sed 's/\^\[/\\e/g' | sed 's/\^\I/\\t/g' )
    printf "echo -e \"%s\"\r\n" "${line}"
  done
}
## BP005: Execute the entrypoint
main "$@"
